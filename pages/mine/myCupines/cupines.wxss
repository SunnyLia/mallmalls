/* 头部导航栏 */
@import "../template/template.wxss";
.orderNav {
  background-color: #fff;
  width: 100%;
  height: 1.6rem;
  line-height: 1.6rem;
  font-size: 0.6rem;
}

.orderNav view {
  width: 33.3333%;
  float: left;
  height: 100%;
  text-align: center;
}

.orderNav view.active text {
  color: #0aa082;
  border-bottom: 1px solid #0aa082;
  padding-bottom: 0.2rem;
}


